{
  "steps": [
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "git", "clone", "https://github.com/istio/istio", "-b", "$_BRANCH", "--depth", "1" ]
    },
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "istio/release/gcb/generate_manifest.sh", "$_GCS_RELEASE_TOOLS_PATH" ]
    }
  ],
  "options": {
    "machineType": "N1_HIGHCPU_8"
  },
  "timeout": "3600s",
}
